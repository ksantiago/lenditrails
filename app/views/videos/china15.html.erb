<%= title "LendIt China 2015 Videos" %>
<% meta_description "Videos of the LendIt China 2015 Conference" %>
<% meta_keywords "p2p, online lending, peer-to-peer, LendIt, China, videos, presentations" %>
<% body_class "vid-index vid-conf-index" %>

<div class="general-background">
  <div class="row">
    <div class="columns">
      <div class="back">
        <a href="/videos">Videos Main</a>
      </div>

      <h1>LendIt China 2015 Videos</h1>
      <h3>Webinar</h3>
      <ul class="small-block-grid-1 medium-block-grid-3 large-block-grid-4">
        <% @webinar.each do |video| %>
        <li>
          <%= link_to china15_video_path(video), class: 'video-item' do %>
          <div class="video-image">
            <img src="<%= video.img %>" alt="<%= video.title %>">
          </div>
          <div class="video-title">
            <%= raw video.title %>
          </div>
          <% end %>
        </li>
        <% end %>

      </ul>



    </div>
  </div>
</div>
